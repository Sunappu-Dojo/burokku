{
    "private": true,
    "engines": {
        "node": ">=12.13"
    },
    "scripts": {
        "sw": "npx babel src/js/service-worker.js --out-file public/block-service-worker.js",
        "sw-watch": "npx babel src/js/service-worker.js --watch --out-file public/block-service-worker.js",
        "dev": "npm run development",
        "development": "node_modules/webpack/bin/webpack.js --node-env=development --progress --config=webpack.config.js",
        "serve": "node_modules/webpack/bin/webpack.js serve --node-env=development --progress --config=webpack.config.js",
        "prod": "npm run production && npm run sw",
        "production": "node_modules/webpack/bin/webpack.js --node-env=production --config=webpack.config.js",
        "test": "npx cypress run",
        "test-gui": "npx exec cypress open",
        "test-watch": "npx nodemon -e html,js,css,scss --ignore 'src/' --exec cypress run",
        "lint": "npx eslint src/js/**/*",
        "lint-fix": "npx eslint src/js/**/* --fix"
    },
    "dependencies": {
        "idb-keyval": "^5",
        "normalize.css": "^8.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.12",
        "@babel/core": "^7.12",
        "@babel/eslint-parser": "^7.14.9",
        "@babel/plugin-proposal-private-methods": "^7.14.5",
        "@babel/preset-env": "^7.12",
        "@fullhuman/postcss-purgecss": "^4",
        "@soda/friendly-errors-webpack-plugin": "^1.8.0",
        "babel-loader": "^8.1",
        "clean-webpack-plugin": "^3.0",
        "copy-webpack-plugin": "^9",
        "css-loader": "^6",
        "cssnano": "^5",
        "cypress": "^7",
        "cypress-dotenv": "^1.2.2",
        "dotenv": "^8",
        "double-dash.scss": "^0.8",
        "easings.scss": "^0.3",
        "eslint": "^7.32.0",
        "eslint-webpack-plugin": "^2.5.4",
        "family.scss": "^1.0",
        "hsl.scss": "^1.0.1",
        "mini-css-extract-plugin": "^2",
        "node-sass": "^7",
        "nodemon": "^2.0.4",
        "postcss": "^8.2.4",
        "postcss-loader": "^6",
        "postcss-preset-env": "^6.7",
        "postcss-safe-area": "0.1",
        "postcss-short-size": "^4.0",
        "sass-loader": "^12",
        "v.scss": "^1.2.1",
        "webpack": "^5.48.0",
        "webpack-build-notifier": "^2.1",
        "webpack-cli": "^4.5",
        "webpack-dev-server": "4.0.0-rc.0",
        "webpack-nano": "^1.1.1"
    }
}
