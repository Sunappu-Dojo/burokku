        "@fullhuman/postcss-purgecss": "^5",
        "@size-limit/preset-app": "^9.0.0",
        "browserslist-to-esbuild": "^1.2.0",
        "cssnano": "^6",
        "double-dash.scss": "^1",
        "eslint": "^8",
        "eslint-formatter-codeframe": "^7.32.1",
        "family.scss": "^1.0",
        "hsl.scss": "^1.0.1",
        "postcss": "^8.2.4",
        "postcss-preset-env": "^9",
        "postcss-short-size": "^4.0",
