{
    "private": true,
    "engines": {
        "node": ">=10.13"
    },
    "scripts": {
        "sw": "cross-env NODE_ENV=production npx babel src/js/service-worker.js --out-file public/block-service-worker.js",
        "sw-watch": "cross-env NODE_ENV=development npx babel src/js/service-worker.js --watch --out-file public/block-service-worker.js",
        "dev": "npm run development && npm run sw",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --config=webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=webpack.config.js",
        "prod": "npm run production && npm run sw",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --config=webpack.config.js",
        "cypress-watch": "npx nodemon -e html,js,css,scss --ignore 'src/' --exec cypress run"
    },
    "dependencies": {
        "idb-keyval": "^3.2",
        "normalize.css": "^8.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/preset-modules": "^0.1.4",
        "@fullhuman/postcss-purgecss": "^2.2.0",
        "babel-preset-minify": "^0.4.3",
        "browser-sync": "^2.26",
        "browser-sync-webpack-plugin": "^2.3",
        "clean-webpack-plugin": "^3.0",
        "copy-webpack-plugin": "^7",
        "cross-env": "^7.0",
        "css-loader": "^5",
        "cssnano": "^4.1",
        "cypress": "^5.1.0",
        "cypress-dotenv": "^1.2.2",
        "dotenv": "^8.2",
        "double-dash.scss": "^0.7",
        "easings.scss": "^0.3",
        "family.scss": "^1.0",
        "friendly-errors-webpack-plugin": "^1.7",
        "hsl.scss": "^1.0",
        "mini-css-extract-plugin": "^1.3",
        "node-sass": "^4.14",
        "nodemon": "^2.0.4",
        "postcss": "^8.2",
        "postcss-loader": "^4.1",
        "postcss-preset-env": "^6.7",
        "postcss-safe-area": "0.0.2",
        "postcss-short-size": "^4.0",
        "sass-loader": "^10.1",
        "v.scss": "^1.2",
        "webpack": "^5.11",
        "webpack-build-notifier": "^2.1",
        "webpack-cli": "^4.3"
    }
}
