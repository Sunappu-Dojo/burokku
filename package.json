{
    "private": true,
    "engines": {
        "node": ">=8.0"
    },
    "scripts": {
        "sw": "cross-env NODE_ENV=production npx babel src/js/service-worker.js --out-file public/block-service-worker.js",
        "sw-watch": "cross-env NODE_ENV=development npx babel src/js/service-worker.js --watch --out-file public/block-service-worker.js",
        "dev": "npm run development && npm run sw",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=webpack.config.js",
        "prod": "npm run production && npm run sw",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=webpack.config.js"
    },
    "dependencies": {
        "idb-keyval": "^3.2.0",
        "normalize.css": "^8.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.7",
        "@babel/core": "^7.7",
        "@babel/plugin-syntax-dynamic-import": "^7.7",
        "@babel/preset-modules": "^0.1",
        "@fullhuman/postcss-purgecss": "^2.2.0",
        "babel-loader": "^8.0",
        "babel-preset-minify": "^0.4.3",
        "browser-sync": "^2.26",
        "browser-sync-webpack-plugin": "^2.2",
        "clean-webpack-plugin": "^3.0",
        "copy-webpack-plugin": "^6",
        "cross-env": "^7.0",
        "css-loader": "^3.2",
        "cssnano": "^4.1",
        "dotenv": "^8.2",
        "double-dash.scss": "^0.6.1",
        "easings.scss": "^0.3",
        "family.scss": "^1.0",
        "file-loader": "^6.0",
        "friendly-errors-webpack-plugin": "^1.7",
        "hsl.scss": "^1.0",
        "mini-css-extract-plugin": "^0.9",
        "node-sass": "^4.14.1",
        "postcss-loader": "^3.0",
        "postcss-preset-env": "^6.7",
        "postcss-safe-area": "0.0.2",
        "postcss-short-size": "^4.0.0",
        "sass-loader": "^8.0",
        "v.scss": "^1.1",
        "webpack": "^4.41",
        "webpack-build-notifier": "^2.0",
        "webpack-cli": "^3.3",
        "webpack-merge": "^4.2"
    }
}
